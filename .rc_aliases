# vim: set ft=sh
alias ..="cd .."
alias ...="cd ../.."
alias -- -="cd -"
alias l='ls -G -a'
alias ls='ls -G -a'
alias tmux='tmux -u'
alias t='tmux'
alias g='git'
alias tig='tig --all'
alias be="bundle exec"
alias ctags_rails='ctags --languages=ruby --exclude=".git*" --exclude="log" -R . $(bundle list --paths)'
alias notesync='sh $PROJECTS/src/github.com/kazukgw/Note/bin/sync'
alias vimrc='vim ~/.vimrc'
alias say='say -v Samantha'

alias ctags_objc="ctags --languages=objectivec --langmap=objectivec:.h.m"

# 起動中のアプリ一覧
alias lsapp="ps aux | grep -v grep | grep Applications | grep -o -e 'MacOS\/.*' | awk '{print \$1}' | sed -e 's/MacOS\///'"

alias vi=vim
alias v=vim

alias youtube-sound="youtube-dl --extract-audio --audio-format mp3"
